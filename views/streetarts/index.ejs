<% layout("layouts/boilerplate") %>
    <h1>
            All the street arts
    </h1>

    <div>
        <a href="/street-arts/create">
            Add new Street Art
        </a>
    </div>

    <ul>
        <% for (let streetart of streetarts) { %>
            <li>
                <a href="/street-arts/<%= streetart._id %>"> 
                    <%= streetart.title %>
                </a>
            </li>
        <% } %>
    </ul>